{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Mayhap/Mayhap.csproj"
          ],
          "src": "{{sourceRoot}}"
        }
      ],
      "dest": "{{reference}}",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": { "TargetFramework": "netstandard1.6" }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "{{reference}}/**.yml",
          "{{reference}}/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "bin/**",
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}